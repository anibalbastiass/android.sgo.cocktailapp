    <application android:fullBackupContent="true">
        <activity android:name="com.anibalbastias.android.cocktailapp.cocktail.feature.CocktailActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
    </application>

</manifest>
